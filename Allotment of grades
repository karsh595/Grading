#include<stdio.h>
int main()
{
	int i;
	printf("Enter your marks :");
	scanf("%d",&i);
	
	if(i<=100 && i>=85)
	{
		printf("Grade A");
	}
	else if(i<=84 && i>=70)
	{
		printf("Grade B");
	}
	else if(i<=69 && i>=55)
	{
		printf("Grade C");
	}
	else if(i<=54 && i>=40)
	{
		printf("Grade D");
	}
	else if(i<40)
	{
		printf("Grade F");
	}
	else
	{
		printf("error");
	}
	return 0;
}
